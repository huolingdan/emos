<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ma.db.dao.SysConfigDao">
  <select id="selectAllParam" resultType="com.ma.db.pojo.SysConfig">
    select param_key,param_value from sys_config where `status`=1;
  </select>

  <select id="selectTest" resultType="HashMap">
    select param_key,param_value from sys_config where `status`=1;
  </select>
</mapper>